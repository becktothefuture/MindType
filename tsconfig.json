{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "rootDir": ".",
    "outDir": "dist",
    "lib": ["ES2024"],
    "types": ["node", "vitest/globals"],
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist", "e2e/**", "web-demo/**"]
}
